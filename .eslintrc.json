{
  "extends": "airbnb",
  "overrides": [
    {
      "files": [
        "**/*spec.js*",
        "**/*test.js*"
      ],
      "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-undef": "off",
        "no-unused-expressions": "off"
      }
    },
    {
      "files": [
        "**/*stories.js*"
      ],
      "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console":"off"
      }
    }
  ]
}
